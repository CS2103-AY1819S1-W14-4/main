<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>

<TableView fx:id="itemTableView" styleClass="background" stylesheets="@DarkTheme.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <columns>
      <TableColumn fx:id="name" editable="false" maxWidth="500.0" minWidth="100.0" prefWidth="-1.0" text="Name" />
      <TableColumn fx:id="price" editable="false" maxWidth="500.0" minWidth="100.0" prefWidth="-1.0" text="Price" />
      <TableColumn fx:id="quantity" editable="false" maxWidth="500.0" minWidth="120.0" prefWidth="-1.0" text="Quantity" />
      <TableColumn fx:id="sku" editable="false" maxWidth="500.0" minWidth="100.0" prefWidth="-1.0" text="Sku" />
      <TableColumn fx:id="tag" editable="false" maxWidth="500.0" minWidth="100.0" prefWidth="-1.0" text="Tags" />
   </columns>
   <columnResizePolicy>
      <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
   </columnResizePolicy>
</TableView>
