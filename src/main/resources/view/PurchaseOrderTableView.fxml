<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.StackPane?>

<StackPane styleClass="background" stylesheets="@DarkTheme.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TableView fx:id="purchaseOrderTableView" styleClass="background" stylesheets="@DarkTheme.css">
         <columns>
            <TableColumn fx:id="sku" editable="false" maxWidth="500.0" minWidth="100.0" prefWidth="-1.0" text="SKU" />
            <TableColumn fx:id="quantity" editable="false" maxWidth="500.0" minWidth="120.0" prefWidth="-1.0" text="Quantity" />
            <TableColumn fx:id="date" editable="false" maxWidth="500.0" minWidth="100.0" prefWidth="-1.0" text="Date" />
            <TableColumn fx:id="supplier" editable="false" maxWidth="500.0" minWidth="100.0" prefWidth="-1.0" text="Supplier" />
            <TableColumn fx:id="status" editable="false" maxWidth="500.0" minWidth="100.0" prefWidth="-1.0" text="Status" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</StackPane>
